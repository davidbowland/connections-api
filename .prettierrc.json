{
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^(\\.+/|@assets|@config|@events|@handlers|@services|@types|@utils)"
  ],
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "printWidth": 100,
  "semi": false,
  "singleQuote": true
}
